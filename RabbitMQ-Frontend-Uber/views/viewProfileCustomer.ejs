<div ng-controller="viewProfileController">
<div class="container" style="padding-top: 60px;">
  <h1 class="page-header">Customer Profile</h1>
  <div class="row">
    <div class="col-md-8 col-sm-6 col-xs-12 personal-info">    
      <h3>Personal info</h3>
      <form class="form-horizontal" role="form">
        <div class="form-group">
          <label class="col-lg-3 control-label">Name:</label>
          <div class="col-lg-8">
            <label> {{customerProfile.firstname}} {{customerProfile.lastname}}</label>
          </div>
        </div>
        <div class="form-group">
          <label class="col-lg-3 control-label">Mobile no:</label>
          <div class="col-lg-8">
            <label>{{customerProfile.mobileno}}</label>
          </div>
        </div>
        <div class="form-group">
          <label class="col-lg-3 control-label">Email:</label>
          <div class="col-lg-8">
            <label>{{customerProfile.email}}</label>
          </div>
        </div>
        <div class="form-group">
          <label class="col-lg-3 control-label">SSN</label>
          <div class="col-lg-8">
            <label>{{customerProfile.ssn}}</label>
          </div>
        </div>
        <div class="form-group">
          <label class="col-lg-3 control-label">Rating</label>
          <div class="col-lg-8">
            <label>{{customerProfile.rating}}</label>
          </div>
        </div>
      </form>
    </div>
  </div>
</div>
</div>